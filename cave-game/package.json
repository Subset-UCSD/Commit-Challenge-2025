{
	"license": "MIT",
	"scripts": {
		"build": "node --experimental-strip-types build.ts build",
		"dev": "node --experimental-strip-types build.ts watch",
		"runnt": "esbuild --bundle --platform=node --packages=external --outdir=dist --format=esm not/bleh.ts && node dist/bleh.js",
		"format": "run-s format:*",
		"format:lint": "eslint --fix",
		"format:prettier": "prettier -w . '!**/*.{js,frag,vert}' --log-level=warn",
		"start": "node ./dist/index.js",
		"typecheck": "tsc --noEmit --skipLibCheck"
	},
	"type": "module",
	"dependencies": {
		"cannon-es": "^0.20.0",
		"express": "^4.21.2",
		"gl-matrix": "^3.4.3",
		"mat4-interpolate": "^1.0.4",
		"matter-js": "^0.20.0",
		"peerjs": "^1.5.4",
		"ws": "^8.18.1"
	},
	"devDependencies": {
		"@types/express": "^5.0.0",
		"@types/matter-js": "^0.19.8",
		"@types/node": "^22.13.4",
		"@types/ws": "^8.5.14",
		"esbuild": "^0.25.5",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unused-imports": "^4.1.4",
		"nodemon": "^3.1.9",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.2",
		"typescript-eslint": "^8.25.0"
	}
}
